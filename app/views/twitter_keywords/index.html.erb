<h4><%= I18n.t("twitter_keywords") %></h4>

<table>
  <tr>
    <th><%= I18n.t("twitter_keywords_index") %></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @twitter_keywords.each do |twitter_keyword| %>
  <tr>
    <td><%= twitter_keyword.name %></td>
    <td><%= link_to I18n.t("show"), twitter_keyword %>|</td>
    <td><%= link_to I18n.t("edit"), edit_twitter_keyword_path(twitter_keyword) %>|</td>
    <td><%= link_to I18n.t("delete"), twitter_keyword, method: :delete, data: { confirm: I18n.t("confirm_delete") } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to I18n.t("new_twitter_keyword"), new_twitter_keyword_path %>
